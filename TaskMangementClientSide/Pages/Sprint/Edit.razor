@page "/editSprint"
@using TaskMangementClientSide.Component.Sprint
@inject NavigationManager Nav


<EditSprintcomponent OnUpdated="HandleSaved" OnCancel="HandleCancel" />



@code {
    private void HandleSaved()
    {
        Nav.NavigateTo("/sprints", forceLoad: true);

    }
    private void HandleCancel()
    {
        Nav.NavigateTo("/sprints");
    }

}
